<div class="tab-pane fade" id="admin-products">
    <div class="row pb-3">
        <div class="col-12">
            <% products.forEach(product => { %>
                <div class="row py-2">
                    <div class="col-2">
                        <img width="100" height="100" src="<%= product.images[0].url %>" alt="<%= product.images[0].alt %>">
                    </div>
                    <div class="col-4">
                        <h4><%= product.title %></h4>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</div>