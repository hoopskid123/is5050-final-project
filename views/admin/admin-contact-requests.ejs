<div class="tab-pane fade" id="admin-contacts">
    <div class="row pb-3">
        <div class="col-12">
            <% contacts.forEach(contact => { %>
                <div class="row py-2">
                    <div class="col-12">
                        <h4><%= contact.subject %></h4>
                        <p><%= contact.message %></p>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</div>