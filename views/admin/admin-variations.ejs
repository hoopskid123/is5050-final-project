<div class="tab-pane fade" id="admin-variations">
    <div class="row pb-3">
        <div class="col-12">
            <% variations.forEach(variation => { %>
                <div class="row py-2">
                    <div class="col-4">
                        <h4><%= `${variation.name} (${variation.slug})` %></h4>
                    </div>
                </div>
            <% }) %>
        </div>
    </div>
</div>